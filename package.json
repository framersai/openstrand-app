  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export:static": "next build && next export",
    "electron:dev": "concurrently -k -n NEXT,ELECTRON \"next dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "electron-builder -c electron-builder.yml",
    "start": "next start",
    "lint": "next lint --quiet || true",
    "brand:export": "tsx scripts/export-logo-assets.ts",
    "type-check": "tsc --noEmit",
    "format": "node ../scripts/run-prettier.mjs --write",
    "lint:fix": "next lint --fix",
    "format:check": "node ../scripts/run-prettier.mjs --check",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:coverage": "vitest run --coverage",
    "docs:types": "node scripts/generate-types-docs.js",
    "sitemap": "node scripts/generate-sitemap.js",
    "postbuild": "npm run sitemap"
  },
